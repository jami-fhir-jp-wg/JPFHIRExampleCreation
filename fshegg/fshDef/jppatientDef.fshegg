* address[ARGnmidx].postalCode = "ARGpostal"
* address[ARGnmidx].text = "ARGtext"
* address[ARGnmidx].country = "JP"
#endbigdef

#bigdef $07or_住所分割(ARGnmidx,ARGpostal,ARGtext,ARGstate,ARGcity,ARGline)
* address[ARGnmidx].text = "ARGtext"
* address[ARGnmidx].line = "ARGline"
* address[ARGnmidx].city = "ARGcity"
* address[ARGnmidx].state = "ARGstate"
* address[ARGnmidx].postalCode = "ARGpostal"
* address[ARGnmidx].country = "JP"
#endbigdef

#bigdef $07or_自宅住所分割(ARGnmidx,ARGpostal,ARGtext,ARGstate,ARGcity,ARGline)
